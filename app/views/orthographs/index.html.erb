<h1>Listing orthographs</h1>

<table>
  <tr>
  </tr>

<% for orthograph in @orthographs %>
  <tr>
    <td><%= link_to 'Show', orthograph %></td>
    <td><%= link_to 'Edit', edit_orthograph_path(orthograph) %></td>
    <td><%= link_to 'Destroy', orthograph, data: { :confirm => 'Are you sure?' }, :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New orthograph', new_orthograph_path %>
